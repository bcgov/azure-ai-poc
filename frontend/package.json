{
  "name": "quickstart-openshift-react-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.17.1"
  },
  "scripts": {
    "dev": "vite --host",
    "clean": "rimraf ./node_modules/.vite",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "deploy": "npm i --ignore-scripts --no-update-notifier --omit=dev && npm run build",
    "preview": "vite preview",
    "test:unit": "vitest --mode test",
    "test:cov": "vitest run --mode test --coverage",
    "build": "vite build"
  },
  "dependencies": {
    "@bcgov/bc-sans": "^2.1.0",
    "@bcgov/design-system-react-components": "^0.5.1",
    "@popperjs/core": "^2.11.8",
    "@tanstack/react-router": "^1.114.12",
    "@tanstack/router-plugin": "^1.114.27",
    "@types/node": "^22.13.11",
    "@vitejs/plugin-react": "^4.3.4",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "keycloak-js": "^26.2.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "sass-embedded": "^1.86.0",
    "vite": "^6.2.3",
    "vite-tsconfig-paths": "^5.1.4",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "typescript": "^5.7.3",
    "vitest": "^3.0.8"
  },
  "overrides": {
    "rollup@<4.22.4": "^4.22.4",
    "@types/react": "^19.0.10"
  }
}
